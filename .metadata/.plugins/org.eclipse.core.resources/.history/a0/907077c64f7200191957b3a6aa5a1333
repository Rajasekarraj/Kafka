package com.kafka.example.kafkaconsumer.model;

public class User {
	private String name;
	private String dpt;
	public User() {
		
	}
	public User(String name, String dpt) {
		super();
		this.name = name;
		this.dpt = dpt;
	} 
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getDpt() {
		return dpt;
	}
	public void setDpt(String dpt) {
		this.dpt = dpt;
	}
}
